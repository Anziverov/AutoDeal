@model IEnumerable<TestDeal>
@{
    ViewData["Title"] = "Cars";
}

<h2>Cars</h2>
<table class="table">
    @foreach(var item in Model)
    {
        <tr class="table">
            <td>
                <h2>@Html.ActionLink(item.Header,"DealDetails","Home",item.Id)</h2>
                <ul class="list-inline" >
                    @foreach(string path in (new PicturesConverter(item.Pictures)).ImagerContainer)
                    {
                        <li ><img src="@path" /></li>
                    }
                </ul>
                <p>@item.Price @item.Currency</p>
            </td>
        </tr>
    }
</table>
